package multichat;

import javax.swing.*;

public class MultiChatData {
    // 데이터 제공 객체
    JTextArea msgOut;

    public void addObj(JComponent comp) {
        this.msgOut = (JTextArea)comp;
    }

    public void refreshData(String msg) {
        // JTextArea 에 수신된 메시지 추가
        msgOut.append(msg);
    }
}